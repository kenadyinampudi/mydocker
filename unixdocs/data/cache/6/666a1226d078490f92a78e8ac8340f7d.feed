sudo groupadd -g 300 ftpgis
sudo useradd -s /usr/bin/csh -g sapsys -G sapinst,dba,abap,ftpgis -d /home/c11adm -c &quot;SAP EU System Admin&quot; -m -u 3207 c11adm
